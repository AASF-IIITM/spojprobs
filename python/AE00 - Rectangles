def main():
	t=ii()
	nor=0
	for i in range(1,t+1):
		nor+=nof(i)
	print(nor)
	
def nof(n):
	nf=1
	sr=int(math.sqrt(n))
	for i in range(2,sr+1):
		if n%i==0:
			nf+=1
	return nf

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
		
		
		
		
		
		
		
		
		
		
		
		
	
main()		
	
