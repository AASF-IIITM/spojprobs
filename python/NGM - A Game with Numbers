MOD=10**9+7
INF=float('+inf')


def ii():
    return int(raw_input())
def mi():
    return map(int, raw_input().split())
def li():
    return [int(i) for i in raw_input().split()]

"-----------------------------------------------"
def main():
    n=ii()
    if n%10!=0:
        print('1')
        print(n%10)
    else:
        print('2')













main()
